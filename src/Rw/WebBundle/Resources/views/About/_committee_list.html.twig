<div class="page-header">
    <h1 id="tables">{{ header|raw }}</h1>
</div>
    <table class="table table-striped">
        <thead>
            <tr>
                <th width="30%">Position</th>
                <th width="30%">Name</th>
                <th width="25%">Phone</th>
                <th width="25%">Email</th>
            </tr>
        </thead>
        <tbody>
            {% for c in committee %}
                <tr>
                    <td>{{ c.getPosition }}</td>
                    <td>
                        {% if c.getName != null %}
                            {{ c.getName }}
                        {% else %}
                            <em class="text-muted">Position Open</em>
                        {% endif %}
                    </td>
                    <td>
                        {% if c.getPhone != null %}
                            <a href="tel:{{ c.getPhone }}">{{ c.getPhone }}</a>
                        {% else %}
                            <em class="text-muted">None</em>
                        {% endif %}
                    </td>
                    <td>
                        {% if c.getEmail != null %}
                            <a href="mailto:{{ c.getEmail }}">{{ c.getEmailShort }}</a>
                        {% else %}
                            <em class="text-muted">None</em>
                        {% endif %}
                    </td>
                </tr>
            {% else %}
                <tr>
                    <td colspan="3">No committee members available at this time.</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
